{
    "plugins": ["import"],

    "rules": {
        "import/no-unused-modules": [
            "error",
            {
                "missingExports": false,
                "unusedExports": true
            }
        ]
    },

    "overrides": [
        {
            "files": ["*.json"],
            "extends": ["plugin:json/recommended"]
        }
    ]
}
